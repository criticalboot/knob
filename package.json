{
  "name": "ng2-knob",
  "version": "1.1.0",
  "description": "Angular 2 Knob Component",
  "scripts": {
    "start": "npm run build && concurrently \"tsc -w\" \"lite-server\" ",
    "e2e": "npm run build && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "npm run build && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "npm run build && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build": "rm -rf lib && tsc -p src && tsc",
    "prepublish": "npm run ngc-build",
    "postinstall": "ngcc",
    "ngc-build": "rm -rf lib && rm -rf \"src/node_modules\" && \"node_modules/.bin/ngc\" -p tsconfig-aot.json"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spheras/knob.git"
  },
  "author": "Jos√© Amuedo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spheras/knob/issues"
  },
  "homepage": "https://github.com/spheras/knob#readme",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/compiler-cli": "^10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/platform-server": "^10.0.3",
    "@angular/router": "~10.0.3",
    "@types/jasmine": "^3.5.11",
    "@types/node": "^14.0.20",
    "@types/selenium-webdriver": "^4.0.9",
    "angular-in-memory-web-api": "~0.11.0",
    "canonical-path": "1.0.0",
    "concurrently": "^5.2.0",
    "core-js": "^3.6.5",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.5.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "protractor": "^7.0.0",
    "reflect-metadata": "^0.1.8",
    "rimraf": "^3.0.2",
    "rxjs": "6.6.0",
    "systemjs": "6.3.3",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6",
    "webdriver-manager": "12.1.7",
    "zone.js": "^0.10.3"
  }
}
